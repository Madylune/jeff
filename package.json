{
  "name": "jeff",
  "description": "JSON Editor File Format",
  "authors": "Julie Nginn",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "init:recipe": "cp ./templates/example.recipe.json ./templates/recipe.json",
    "init:resume": "cp ./templates/example.resume.json ./templates/resume.json",
    "write:recipe": "node ./scripts/jsonToPdf.js",
    "write:resume": "node ./scripts/jsonToPdf.js",
    "watch:recipe": "nodemon ./scripts/jsonToPdf.js",
    "watch:resume": "nodemon ./scripts/jsonToPdf.js"
  },
  "dependencies": {
    "nodemon": "2.0.22",
    "pdfkit": "0.13.0"
  }
}
